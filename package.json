{
  "name": "react-native-duofy-ui-components",
  "version": "1.0.15",
  "private": false,
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "sourceMaps": true
        }
      ],
      [
        "typescript"
      ]
    ]
  },
  "main": "lib/commonjs/index.js",
  "react-native": "src/index.ts",
  "types": "src/types.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "yarn storybook-generate && react-native start",
    "test": "jest",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "postinstall": "husky install",
    "prepack": "rm -rf lib *.tgz && pinst --disable && bob build",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "classnames": "^2.3.2",
    "css-loader": "^6.8.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.72.1",
    "react-native-heroicons": "^3.2.0",
    "react-native-sass-transformer": "^2.0.0",
    "react-native-svg": "^13.10.0",
    "sass": "^1.63.6",
    "style-loader": "^3.3.3",
    "twrnc": "^3.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.22.7",
    "@babel/runtime": "^7.20.0",
    "@react-native-async-storage/async-storage": "^1.19.0",
    "@react-native-community/datetimepicker": "^7.4.0",
    "@react-native-community/slider": "^4.4.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.7",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-ondevice-actions": "^6.5.4",
    "@storybook/addon-ondevice-controls": "^6.5.4",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/react-native": "^6.5.4",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^3.0.0",
    "@types/metro-config": "^0.76.3",
    "@types/node-sass": "^4.11.3",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.3.0",
    "babel-plugin-react-native-classname-to-style": "^1.2.2",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-plugin-transform-scss": "^1.2.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.5",
    "pinst": "^3.0.0",
    "postcss": "^8.4.25",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^12.0.0",
    "prettier": "^2.4.1",
    "react-dom": "18.2.0",
    "react-native-builder-bob": "^0.21.2",
    "react-native-safe-area-context": "^4.6.4",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "sass-loader": "^13.3.2",
    "tailwindcss": "^3.3.2",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "4.8.4",
    "webpack": "^5.88.1"
  },
  "engines": {
    "node": ">=16"
  }
